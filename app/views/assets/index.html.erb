<table width="800">
	<tr>
		<th>Product Id</th>
		<th>Name</th>
		<th>Status</th>
		<th>Assigned To</th>
		<th>Action</th>
	</tr>
	<% @assets.each do |asset| %>
	<tr>
		<td><%= asset.id %></td>
		<td><%= link_to asset.name, asset %></td>
		<td><%= asset.status %></td>
		<td><%= show_employee asset %></td>
		<td><%= link_to 'Edit', edit_asset_path(asset.id.to_s) %></td>
	</tr>
	<% end %>
</table>
